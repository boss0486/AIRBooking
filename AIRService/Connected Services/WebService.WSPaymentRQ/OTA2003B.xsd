<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="http://www.opentravel.org/OTA/2003/05/beta" elementFormDefault="qualified" targetNamespace="http://www.opentravel.org/OTA/2003/05/beta" version="3.0.0" id="OTA2003B" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="PaymentRQ">
    <xs:annotation>
      <xs:documentation>Root element for any Payment service request</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Action">
          <xs:complexType>
            <xs:simpleContent>
              <xs:extension base="xs:string">
                <xs:attribute name="Type" type="xs:string" />
              </xs:extension>
            </xs:simpleContent>
          </xs:complexType>
        </xs:element>
        <xs:element name="POS" type="POS_Type" />
        <xs:element minOccurs="0" name="ServiceIndicator">
          <xs:complexType>
            <xs:attribute name="MultiFOPsProcessInd" type="xs:string" />
            <xs:attribute name="FraudScreenInd" type="xs:boolean" />
          </xs:complexType>
        </xs:element>
        <xs:element name="MerchantDetail" type="MerchantDetailType" />
        <xs:element minOccurs="0" name="AuthenticationDetail" type="AuthenticationDetailType" />
        <xs:element minOccurs="0" name="OrderDetail">
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="0" maxOccurs="99" name="ProductDetail" type="ProductDetailType" />
              <xs:element minOccurs="0" maxOccurs="99" name="PassengerDetail" type="PassengerDetailType" />
              <xs:element minOccurs="0" name="ContactInfo" type="ContactInfoType" />
              <xs:element minOccurs="0" maxOccurs="20" name="FlightDetail" type="FlightDetailType" />
              <xs:element minOccurs="0" maxOccurs="5" name="Hotel" type="HotelType" />
              <xs:element minOccurs="0" maxOccurs="5" name="CarRental" type="CarRentalType" />
              <xs:element minOccurs="0" maxOccurs="5" name="Rail" type="RailType" />
              <xs:element minOccurs="0" name="ShippingAddress" type="AddressType" />
              <xs:element minOccurs="0" name="Remarks" type="xs:string" />
              <xs:element minOccurs="0" name="MCP" type="MCPType" />
            </xs:sequence>
            <xs:attribute name="UniqueBookingID" type="xs:string">
              <xs:annotation>
                <xs:documentation>Identifier for tracking transactions originating from the same booking/PNR</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute name="SessionID" type="xs:string">
              <xs:annotation>
                <xs:documentation>Identifier for tracking transactions from the same session</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute name="OrderID" type="xs:string">
              <xs:annotation>
                <xs:documentation>Identifier for tracking purposes.</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute name="OrderType" type="xs:string">
              <xs:annotation>
                <xs:documentation>O - Original (default)
EE - Even Exchange
EA - Exchange w/ Add Collect
ER - Exchange w/ Refund
R - Refund</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute name="OrderOfferType" type="xs:string" />
            <xs:attribute name="OrderCreateDate" type="xs:date">
              <xs:annotation>
                <xs:documentation>Indicates the creation date of the order in UTC using the following format specified by ISO 8601; YYYY-MM-DD</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute name="RecordLocator" type="xs:string">
              <xs:annotation>
                <xs:documentation>PNR Record locator</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DollarAmountPaid" type="Money" use="optional" />
            <xs:attribute name="InvoiceNumber" type="xs:string" />
            <xs:attribute name="ThirdPartyBookingInd" type="xs:boolean">
              <xs:annotation>
                <xs:documentation>Indicates if the booking was made by a 3rd party. i.e. if the card holder is not one of the passengers, set this to true.</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute name="OneWayInd" type="xs:boolean">
              <xs:annotation>
                <xs:documentation>Set to true if the booking is a one-way trip.</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute name="MultiCityInd" type="xs:boolean">
              <xs:annotation>
                <xs:documentation>Set to true if the booking involves more than 1 destination cities.</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute name="PromotionCode" type="xs:string">
              <xs:annotation>
                <xs:documentation>For BML:
"SAC06M" - 6-month Same As Cash</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ValidatingCarrierCode" type="xs:string" />
            <xs:attribute name="ValidatingCarrierNbr" type="xs:string" />
            <xs:attribute name="OrderExpireDateTime" type="xs:dateTime">
              <xs:annotation>
                <xs:documentation>Indicates the expiration date and time of the order in UTC using the following format specified by ISO 8601; YYYY-MM-DDThh:mm:ssZ with time values using the 24 hour clock (e.g. 20 November 2017, 1:59:38 pm UTC becomes 2017-11-20T13:59:38Z).</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ClearingSequence" type="xs:string">
              <xs:annotation>
                <xs:documentation>Indicates the tracking clearing sequence number</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ClearingCount" type="xs:string">
              <xs:annotation>
                <xs:documentation>Indicates the total of clearing count</xs:documentation>
              </xs:annotation>
            </xs:attribute>
          </xs:complexType>
        </xs:element>
        <xs:element minOccurs="0" maxOccurs="10" name="AccountDetail" type="AccountDetailType" />
        <xs:element minOccurs="0" maxOccurs="10" name="PaymentDetail" type="PaymentDetailType" />
        <xs:element minOccurs="0" name="CustomerDetail">
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="0" name="Address" type="AddressType" />
              <xs:element minOccurs="0" name="EmailAddress" type="xs:string" />
              <xs:element minOccurs="0" maxOccurs="3" name="PhoneNumber" type="PhoneType" />
              <xs:element minOccurs="0" name="CustomerID" type="PersonID_Type" />
              <xs:element minOccurs="0" name="LoyaltyMemberDetail" type="LoyaltyMembershipDetailType" />
            </xs:sequence>
            <xs:attributeGroup ref="PersonNameGroup" />
            <xs:attribute name="DOB" type="xs:date" use="optional">
              <xs:annotation>
                <xs:documentation>1980-07-31</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Gender" type="xs:string" use="optional">
              <xs:annotation>
                <xs:documentation>M/F</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Type" type="xs:string" use="optional">
              <xs:annotation>
                <xs:documentation>N - New
E - Existing</xs:documentation>
              </xs:annotation>
            </xs:attribute>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="SystemDateTime" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation xml:lang="en">Indicates the creation date and time of the message in UTC format</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="SabreTransactionID" type="xs:string" use="optional">
        <xs:annotation>
          <xs:documentation xml:lang="en">Unique identifier generated by PWS</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="DeferredPaymentID" />
      <xs:attribute name="ReturnSupplierMessagesInd" type="xs:boolean" use="optional">
        <xs:annotation>
          <xs:documentation>For test environment only. If set to true, supplier request/response will be included in PaymentRS</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="RetransmissionIndicator" type="xs:boolean" use="optional">
        <xs:annotation>
          <xs:documentation xml:lang="en">When true, indicates the message is being re-sent after a failure. It is recommended that this attribute is used (i.e., set to TRUE) only when a message is retransmitted.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="POSActionCode" type="xs:string" use="optional" />
      <xs:attribute name="Version" type="xs:string" use="required">
        <xs:annotation>
          <xs:documentation>Schema version (4.0.0)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="PaymentRS">
    <xs:annotation>
      <xs:documentation>Payment Service response</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="POS" type="POS_Type" />
        <xs:element minOccurs="0" name="MerchantDetail" type="MerchantDetailType" />
        <xs:element minOccurs="0" name="AuthenticationDetail" type="AuthenticationDetailType" />
        <xs:element minOccurs="0" name="OrderDetail">
          <xs:complexType>
            <xs:attribute name="RecordLocator" type="xs:string" />
            <xs:attribute name="OrderID" type="xs:string" />
          </xs:complexType>
        </xs:element>
        <xs:sequence>
          <xs:element name="Result">
            <xs:complexType>
              <xs:attribute name="ResultCode" type="xs:string" use="required" />
              <xs:attribute name="Description" type="xs:string" use="optional" />
              <xs:attribute name="POSActionCode" type="xs:string" use="optional" />
              <xs:attribute name="ErrorCode" type="xs:string" use="optional" />
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="99" name="Results">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" name="FOP">
                  <xs:complexType>
                    <xs:complexContent mixed="false">
                      <xs:extension base="FOPType" />
                    </xs:complexContent>
                  </xs:complexType>
                </xs:element>
                <xs:choice>
                  <xs:element minOccurs="0" name="AccountValidationResult">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element minOccurs="0" name="CSC_Result" type="CSC_ResultType" />
                        <xs:element minOccurs="0" name="AVS_Result" type="AVS_ResultType" />
                        <xs:element minOccurs="0" name="Remarks" type="RemarksType" />
                      </xs:sequence>
                      <xs:attribute name="ResultCode" type="xs:string" use="required" />
                      <xs:attribute name="Description" type="xs:string" use="optional" />
                      <xs:attribute name="ApprovalCode" type="xs:string" use="optional" />
                      <xs:attribute name="SupplierID" type="xs:string" use="optional" />
                      <xs:attribute name="SupplierTransID" type="xs:string" use="optional" />
                      <xs:attribute name="SupplierResponseCode" type="xs:string" use="optional" />
                    </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="AuthorizationResult" type="AuthorizationResultType" />
                  <xs:element minOccurs="0" maxOccurs="10" name="ConfirmAuthResult" type="ConfirmAuthResultType" />
                  <xs:element minOccurs="0" maxOccurs="10" name="CancelAuthResult" type="CancelAuthResultType" />
                  <xs:element minOccurs="0" maxOccurs="10" name="RefundResult" type="RefundResultType" />
                  <xs:element minOccurs="0" maxOccurs="10" name="CancelRefundResult" type="CancelRefundResultType" />
                  <xs:element minOccurs="0" name="RegisterAuthResult" type="RegisterAuthResultType" />
                </xs:choice>
                <xs:element minOccurs="0" name="PaymentDetail" type="PaymentDetailType" />
                <xs:element minOccurs="0" name="FraudScreenResult" type="FraudScreenResultType" />
                <xs:element minOccurs="0" name="TokenizationResult" type="TokenizationResultType" />
                <xs:element minOccurs="0" name="Remarks" type="RemarksType" />
              </xs:sequence>
              <xs:attribute name="FOP_RPH" />
              <xs:attribute name="POSActionCode" type="xs:string" use="optional" />
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:choice>
          <xs:element minOccurs="0" name="BalanceInquiryResult">
            <xs:complexType>
              <xs:attribute name="ResponseCode" type="xs:string" use="required" />
              <xs:attribute name="Description" type="xs:string" use="optional" />
              <xs:attribute name="SupplierTransID" type="xs:string" use="optional" />
              <xs:attribute name="SupplierResponseCode" type="xs:string" use="optional" />
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="VerificationResult">
            <xs:complexType>
              <xs:attribute name="ResponseCode" type="xs:string" use="required" />
              <xs:attribute name="Description" type="xs:string" />
              <xs:attribute name="ApprovedAmount" type="Money" />
              <xs:attribute name="BonusAmount" type="Money" />
              <xs:attribute name="CurrencyCode" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Transaction currency</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="ExchangeRate" type="xs:double" />
              <xs:attribute name="SupplierTransID" type="xs:string" />
              <xs:attribute name="SupplierResponseCode" type="xs:string" />
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="DCC_Result">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="99" name="PassengerDetail" type="PassengerDetailType" />
              </xs:sequence>
              <xs:attribute name="DCC_Offered" type="xs:boolean" use="required">
                <xs:annotation>
                  <xs:documentation>DCC Indicator</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="SupplierResponseCode" type="xs:string" use="optional" />
              <xs:attribute name="SupplierID" type="xs:string" use="optional" />
              <xs:attribute name="SupplierTransID" type="xs:string" use="optional" />
              <xs:attribute name="CardCurrencyCode" type="xs:string" use="optional" />
              <xs:attribute name="CardCurrencyAmount" type="Money" use="optional" />
              <xs:attribute name="CalculatedCardCurrencyAmount" type="Money" use="optional" />
              <xs:attribute name="ExchangeRate" type="xs:double" use="optional" />
              <xs:attribute name="ExchangeRateSourceName" type="xs:string" use="optional" />
              <xs:attribute name="ExchangeRateSourceDateTime" type="xs:dateTime" use="optional" />
              <xs:attribute name="MarginPercentRate" type="xs:double" use="optional" />
              <xs:attribute name="CommissionPercentRate" type="xs:double" use="optional" />
              <xs:attribute name="ReasonCode" type="xs:string" use="optional" />
              <xs:attribute name="Description" type="xs:string" use="optional" />
              <xs:attribute name="DCC_Remarks" type="xs:string" use="optional" />
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="MCP_Result">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="MCP_Offer">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element minOccurs="0" maxOccurs="99" name="PassengerDetail" type="PassengerDetailType" />
                    </xs:sequence>
                    <xs:attribute name="CurrencyCode" type="xs:string" use="optional" />
                    <xs:attribute name="ExchangeRate" type="xs:double" use="optional" />
                    <xs:attribute name="ExchangeRateSourceName" type="xs:string" use="optional" />
                    <xs:attribute name="ExchangeRateSourceDateTime" type="xs:dateTime" use="optional" />
                    <xs:attribute name="ExchangeRateExpireDateTime" type="xs:dateTime" use="optional" />
                    <xs:attribute name="MarginPercentRate" type="xs:double" use="optional" />
                    <xs:attribute name="CommissionPercentRate" type="xs:double" use="optional" />
                    <xs:attribute name="Amount" type="Money" use="optional" />
                    <xs:attribute name="CalculatedAmount" type="Money" use="optional" />
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
              <xs:attribute name="MCP_Offered" type="xs:boolean" use="required">
                <xs:annotation>
                  <xs:documentation>Multi Currency Pricing Indicator</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="SupplierResponseCode" type="xs:string" use="optional" />
              <xs:attribute name="SupplierID" type="xs:string" use="optional" />
              <xs:attribute name="SupplierTransID" type="xs:string" use="optional" />
              <xs:attribute name="ReasonCode" type="xs:string" use="optional" />
              <xs:attribute name="Description" type="xs:string" use="optional" />
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="10" name="AccountValidationResult">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" name="CSC_Result" type="CSC_ResultType" />
                <xs:element minOccurs="0" name="AVS_Result" type="AVS_ResultType" />
              </xs:sequence>
              <xs:attribute name="ResultCode" type="xs:string" use="required" />
              <xs:attribute name="Description" type="xs:string" use="optional" />
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" maxOccurs="10" name="AuthorizationResult" type="AuthorizationResultType" />
          <xs:element minOccurs="0" maxOccurs="10" name="ConfirmAuthResult" type="ConfirmAuthResultType" />
          <xs:element minOccurs="0" maxOccurs="10" name="CancelAuthResult" type="CancelAuthResultType" />
          <xs:element minOccurs="0" maxOccurs="10" name="RefundResult" type="RefundResultType" />
          <xs:element minOccurs="0" maxOccurs="10" name="CancelRefundResult" type="CancelRefundResultType" />
          <xs:element minOccurs="0" name="GetCardResult">
            <xs:complexType>
              <xs:complexContent mixed="false">
                <xs:extension base="GetCardResultType" />
              </xs:complexContent>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="QueryDeferredPaymentResult">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="PaymentDetail" type="PaymentDetailType" />
                <xs:element minOccurs="0" name="AccountDetail" type="AccountDetailType" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="GetBINResult">
            <xs:complexType>
              <xs:attribute name="ResponseCode" type="xs:string" use="required" />
              <xs:attribute name="Description" type="xs:string" />
              <xs:attribute name="CardCode" type="xs:string" use="required" />
              <xs:attribute name="CardBIN" type="xs:string" use="required" />
            </xs:complexType>
          </xs:element>
        </xs:choice>
        <xs:element minOccurs="0" maxOccurs="10" name="CaptureResult">
          <xs:complexType>
            <xs:attribute name="ResponseCode" type="xs:string" use="required" />
            <xs:attribute name="Description" type="xs:string" use="optional" />
            <xs:attribute name="CaptureRemarks" type="xs:string" use="optional" />
            <xs:attribute name="SupplierID" type="xs:string" use="optional" />
            <xs:attribute name="SupplierTransID" type="xs:string" use="optional" />
            <xs:attribute name="SupplierResponseCode" type="xs:string" use="optional" />
          </xs:complexType>
        </xs:element>
        <xs:element minOccurs="0" maxOccurs="10" name="AccountDetail">
          <xs:complexType>
            <xs:complexContent mixed="false">
              <xs:extension base="AccountDetailType">
                <xs:sequence>
                  <xs:element minOccurs="0" maxOccurs="10" name="SubBalance" type="SubBalanceType" />
                  <xs:element minOccurs="0" name="ListOfVouchers">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element maxOccurs="99" name="Voucher" type="VoucherType" />
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                </xs:sequence>
                <xs:attribute name="InternalAccountNbr" type="xs:string" use="optional" />
                <xs:attribute name="Balance" type="Money" use="optional" />
                <xs:attribute name="Usage" type="xs:string" use="optional" />
                <xs:attribute name="ValidFrom" type="xs:date" use="optional" />
                <xs:attribute name="ValidUntil" type="xs:date" use="optional" />
              </xs:extension>
            </xs:complexContent>
          </xs:complexType>
        </xs:element>
        <xs:element minOccurs="0" name="PaymentDetail" type="PaymentDetailType" />
        <xs:element minOccurs="0" name="DeferredPaymentDetail">
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="0" maxOccurs="10" name="FOP" type="FOPType" />
              <xs:choice>
                <xs:element minOccurs="0" name="CardDetail">
                  <xs:complexType>
                    <xs:attribute name="CardCode" type="xs:string" use="required" />
                    <xs:attribute name="CardNumber" type="xs:string" use="optional" />
                    <xs:attribute name="TokenizedCardNumber" type="xs:string" use="optional" />
                    <xs:attribute name="MaskedCardNumber" type="xs:string" use="optional" />
                    <xs:attribute name="ExpireDate" type="xs:string" use="optional" />
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="AccountDetail" type="AccountDetailType" />
              </xs:choice>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element minOccurs="0" name="FraudScreenResult" type="FraudScreenResultType" />
        <xs:element minOccurs="0" name="FraudRemarks">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Remarks1" type="xs:string" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element minOccurs="0" maxOccurs="unbounded" name="TokenizationResults" type="TokenizationResultType" />
        <xs:element minOccurs="0" name="NotificationList">
          <xs:complexType>
            <xs:sequence>
              <xs:element maxOccurs="unbounded" name="Notification">
                <xs:complexType>
                  <xs:attribute name="DeliveryAddress" type="xs:string" />
                  <xs:attribute name="Status" type="xs:string" />
                  <xs:attribute name="DeliveryDate" type="xs:dateTime" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element minOccurs="0" name="Remarks" type="RemarksType" />
      </xs:sequence>
      <xs:attribute name="SystemDateTime" type="xs:dateTime" use="required">
        <xs:annotation>
          <xs:documentation xml:lang="en">Indicates the creation date and time of the message in ISO8601 format</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="SabreTransactionID" type="xs:string" use="optional">
        <xs:annotation>
          <xs:documentation xml:lang="en">A unique identifier</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="DeferredPaymentID" />
      <xs:attribute name="RetransmissionIndicator" type="xs:boolean" use="optional">
        <xs:annotation>
          <xs:documentation xml:lang="en">When true, indicates the message is being re-sent after a failure. It is recommended that this attribute is used (i.e., set to TRUE) only when a message is retransmitted.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="Version" use="required">
        <xs:annotation>
          <xs:documentation>Schema version (e.g. 1.0.1)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string" />
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="TPA_Extensions" type="TPA_ExtensionsType">
    <xs:annotation>
      <xs:documentation xml:lang="en">A placeholder in the schema to allow for additional elements and attributes to be included if required, per Trading Partner Agreement (TPA).</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="AccountDetailType">
    <xs:attribute name="AccountType" type="xs:string" />
    <xs:attribute name="AccountNbr" type="xs:string" use="optional" />
    <xs:attribute name="AccountCode" type="xs:string" use="optional" />
    <xs:attribute name="MaskedAccountNbr" type="xs:string" use="optional" />
    <xs:attribute name="TokenizedAccountNbr" type="xs:string" use="optional" />
    <xs:attribute name="PIN" type="xs:string" />
    <xs:attribute name="AccountHolderName" type="xs:string" use="optional" />
    <xs:attributeGroup ref="UserNameGroup" />
    <xs:attribute name="ExpireDate" type="xs:string" />
    <xs:attribute name="AccountCurrencyCode" type="xs:string">
      <xs:annotation>
        <xs:documentation>Account currency</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="CurrencyCode" type="xs:string">
      <xs:annotation>
        <xs:documentation>Transaction currency</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="IssuerName" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>Name of account issuer such as bank name</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="BusinessIdentifierCode" type="xs:string" use="optional">
      <xs:annotation>
        <xs:documentation>BIC for SEPA</xs:documentation>
