      <xs:annotation>
        <xs:documentation xml:lang="en">Dual currency</xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="mOverride" type="xs:boolean">
      <xs:annotation>
        <xs:documentation xml:lang="en">M override</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
