      </xs:annotation>
    </xs:attribute>
    <xs:attribute name="currency" type="AlphaLength3">
      <xs:annotation>
        <xs:documentation>Currency of Adjustment's Value</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="ValueType">
    <xs:attribute name="value" type="xs:decimal" />
  </xs:complexType>
  <xs:complexType name="PriceRequestOptionsType">
