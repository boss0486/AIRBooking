<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns="http://services.sabre.com/sp/pd/v3_4" xmlns:stlp="http://services.sabre.com/STL_Payload/v02_01" elementFormDefault="qualified" targetNamespace="http://services.sabre.com/sp/pd/v3_4" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import schemaLocation="built-ins/STL2_Payload_v02_01.xsd" namespace="http://services.sabre.com/STL_Payload/v02_01" />
  <xs:import schemaLocation="built-ins/SpCommonSimpleTypes_v3.xsd" namespace="http://services.sabre.com/sp/common/simple/v3" />
  <xs:element name="PassengerDetailsRS">
    <xs:complexType>
      <xs:complexContent mixed="false">
        <xs:extension base="stlp:STL_Response_Payload">
          <xs:sequence>
            <xs:element minOccurs="0" name="ItineraryRef">
              <xs:annotation>
                <xs:documentation>Used to pass the ID of the ended record</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:attribute name="ID" type="xs:string">
                  <xs:annotation>
                    <xs:documentation>Used to return the locator of the ended record</xs:documentation>
                  </xs:annotation>
                </xs:attribute>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" name="TravelItineraryReadRS">
              <xs:annotation>
                <xs:documentation>Used to pass details of the ended record</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" ref="TravelItinerary" />
                  <xs:element minOccurs="0" ref="PriceQuote" />
                  <xs:element minOccurs="0" ref="VCR" />
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="TravelItinerary">
    <xs:annotation>
      <xs:documentation>Reservation details</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" name="AccountingInfo">
          <xs:annotation>
            <xs:documentation>Used to return the LinkCode, type and Id.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="0" name="AccountVendor">
                <xs:complexType>
                  <xs:attribute name="Code" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>Used to return the accounting vendor code associated with the particular accounting line.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="Airline">
                <xs:complexType>
                  <xs:attribute name="Code" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>Used to return the airline vendor code associated with the particular accounting line.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="BaseFare">
                <xs:annotation>
                  <xs:documentation>Contains the Base Fare details. Contains the Base Fare amount, applicable currency code and Applicable Percentage Example: Amount="422"	CurrencyCode="USD"	Percent="0".</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:attribute name="Amount" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>"Amount" is used to return the applicable amount.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="CurrencyCode" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>"CurrencyCode" is used to return the applicable currency code.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="Percent" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>"Percent" is used to return the applicable percentage.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="ChargeCategory" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Used to return the charge category.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="DocumentInfo">
                <xs:complexType>
                  <xs:sequence>
                    <xs:annotation>
                      <xs:documentation>Holds the Document Number information	Example: Number="1234567890"</xs:documentation>
                    </xs:annotation>
                    <xs:element minOccurs="0" name="Document">
                      <xs:complexType>
                        <xs:attribute name="Number" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>Used to return the document number.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="FareApplication" type="xs:string">
                <xs:annotation>
                  <xs:documentation>Holds the Fare application information.	Example: ONE.</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="PaymentInfo">
                <xs:annotation>
                  <xs:documentation>PaymentInfo is used as a container for Payment information details.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element minOccurs="0" name="Commission">
                      <xs:annotation>
                        <xs:documentation>Commission is used as a place holder for commission associated with the particular accounting line	Example: Commission Amount="31.00" Percent="1"</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:attribute name="Amount" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>Used to return the applicable commission amount associated with the particular accounting line.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Percent" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>Used to return the applicable commission percentage associated with the particular accounting line.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="Payment">
                      <xs:annotation>
                        <xs:documentation>Used to return the form of payment.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element minOccurs="0" name="CC_Info">
                            <xs:annotation>
                              <xs:documentation>Used as a container for payment Card details. Hold the payment card type, and the card number.	Example: PaymentCard Code="AX" Number="3XXXXXXXXXX8431".</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element minOccurs="0" maxOccurs="unbounded" name="PaymentCard">
                                  <xs:complexType>
                                    <xs:attribute name="Code" type="xs:string">
                                      <xs:annotation>
                                        <xs:documentation>"Code" is used to return credit card code associated with the particular accounting line.</xs:documentation>
                                      </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="Number" type="xs:string">
                                      <xs:annotation>
                                        <xs:documentation>"Number" is used to return credit card number associated with the particular accounting line.</xs:documentation>
                                      </xs:annotation>
                                    </xs:attribute>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element minOccurs="0" name="Form" type="xs:string">
                            <xs:annotation>
                              <xs:documentation>Used to return the form of payment associated with the particular accounting line.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="PersonName">
                <xs:annotation>
                  <xs:documentation>PersonName is used to return the passenger name associated with the particular accounting line.	Example: NameNumber="1.1" TESTA TEST1".</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:string">
                      <xs:attribute name="NameNumber" type="xs:string">
                        <xs:annotation>
                          <xs:documentation>Used to return the name number.</xs:documentation>
                        </xs:annotation>
                      </xs:attribute>
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="Taxes">
                <xs:annotation>
                  <xs:documentation>Taxes is used as a container for Tax details.
                                        It holds the details for GST, QST and Tax. For GST information about amount, Currency code and Percentage is present for QST and Tax along with information about amount, Currency code,
                                        Percentage Tax code details is also included.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element minOccurs="0" name="GST">
                      <xs:complexType>
                        <xs:attribute name="Amount" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>"Amount" is used to return the applicable amount.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CurrencyCode" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>"CurrencyCode" is used to return the applicable currency code.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Percent" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>"Percent" is used to return the applicable percentage.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="QST">
                      <xs:complexType>
                        <xs:attribute name="Amount" type="xs:string" />
                        <xs:attribute name="CurrencyCode" type="xs:string" />
                        <xs:attribute name="Percent" type="xs:string" />
                        <xs:attribute name="TaxCode" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>"TaxCode" is used to return the tax code.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="Tax">
                      <xs:complexType>
                        <xs:attribute name="Amount" type="xs:string" />
                        <xs:attribute name="CurrencyCode" type="xs:string" />
                        <xs:attribute name="Percent" type="xs:string" />
                        <xs:attribute name="TaxCode" type="xs:string" />
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="Text" type="xs:string">
                <xs:annotation>
                  <xs:documentation>"Text" is used to return miscellaneous free text-related information associated with the	particular ticketing information if applicable	Example: INCLUDES SERVICE CHARGE</xs:documentation>
                </xs:annotation>
              </xs:element>
              <xs:element minOccurs="0" name="TicketingInfo">
                <xs:annotation>
                  <xs:documentation>"TicketingInfo" is used as a container for  Ticketing Information</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element minOccurs="0" name="eTicket">
                      <xs:annotation>
                        <xs:documentation>"eTicket" holds indicator to denote whether or not the flight segment associated with the accounting line is e-ticket eligible	Example: eTicket Ind="false"</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:attribute name="Ind" type="xs:boolean" />
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="Exchange">
                      <xs:annotation>
                        <xs:documentation>"Exchange" holds indicator to denote whether or not the the accounting line relates to an exchange transaction	Example: Exchange Ind="false"</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:attribute name="Ind" type="xs:boolean" />
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="IssueDate" type="date">
                      <xs:annotation>
                        <xs:documentation>"IssueDate" is used to return the issue date associated with the particular accounting line.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="IssueLocation" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>"IssueLocation" is used to return the issue location associated with the particular accounting line if applicable</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="OriginalInvoiceNumber" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>"Number" is used to return the original invoice number associated with the particular accounting line.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" maxOccurs="9" name="OriginalTicketNumber" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>"OriginalTicketNumber" is used to return the original ticket number associated with the particular accounting line.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="TariffBasis" type="xs:string">
                      <xs:annotation>
                        <xs:documentation>"TariffBasis" is used to return tariff basis-related information associated with the particular accounting line.</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="Ticketing">
                      <xs:complexType>
                        <xs:attribute name="ConjunctedCount" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>"ConjunctedCount" is used to return the number of conjuncted eTicket coupons associated with the particular accounting line.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CouponCount" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>"CouponCount" is used to return the number of eTicket coupons associated with the particular accounting line.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="LinkCode" type="xs:string" />
            <xs:attribute name="Type" type="xs:string">
              <xs:annotation>
                <xs:documentation>
                                    Used to return Accounting info type e.g. R - refund, L - discount, B - bulk, G - Air exchange with Refund, A - Air exchange with Add, ADP - supplier/non-accountable document.</xs:documentation>
              </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Id" type="xs:string" />
          </xs:complexType>
        </xs:element>
        <xs:element minOccurs="0" name="CustomerInfo">
          <xs:annotation>
            <xs:documentation>Customer details</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="0" name="Address">
                <xs:annotation>
                  <xs:documentation>Address</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element minOccurs="0" maxOccurs="unbounded" name="AddressLine">
                      <xs:annotation>
                        <xs:documentation>"AddressLine" is used to return customer address information.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:string">
                            <xs:attribute name="Id" type="xs:string">
                              <xs:annotation>
                                <xs:documentation>Reference ID</xs:documentation>
                              </xs:annotation>
                            </xs:attribute>
                            <xs:attribute name="type" type="xs:string">
                              <xs:annotation>
                                <xs:documentation>Indicates type of information ex.
                                                                    O Other
                                                                    N Name
                                                                    A Address
                                                                    C City and State
                                                                    Z Zip code
                                                                    B Business</xs:documentation>
                              </xs:annotation>
                            </xs:attribute>
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="ContactNumbers">
                <xs:annotation>
                  <xs:documentation>Contact numbers</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element minOccurs="0" maxOccurs="unbounded" name="ContactNumber">
                      <xs:annotation>
                        <xs:documentation>Contact number</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element minOccurs="0" maxOccurs="unbounded" name="PersonName">
                            <xs:annotation>
                              <xs:documentation>PersonName is the passenger name.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:string">
                                  <xs:attribute name="NameNumber" type="xs:string">
                                    <xs:annotation>
                                      <xs:documentation>NameNumber is used to return the passenger name number associated with the particular passenger</xs:documentation>
                                    </xs:annotation>
                                  </xs:attribute>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                        <xs:attribute name="LocationCode" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>"LocationCode" is used to return the city code associated to the phone number.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Phone" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>"Phone" is used to return the telephone number.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="RPH" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>"RPH" is used as a reference placeholder.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Id" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>Reference ID</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="unbounded" name="CustLoyalty">
                <xs:annotation>
                  <xs:documentation>Frequent flyer information</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:attribute name="TravelingCarrierCode" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>"TravelingCarrierCode" is used to return the traveling airline code.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="MembershipID" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>"MembershipID" is used to return the passenger's frequent flyer number.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="NameNumber" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>"NameNumber" is used to associate the particular frequent flyer number to a particular passenger within the record.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ProgramID" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>"ProgramID" is used to return the passenger's frequent flyer airline code.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="RPH" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>"RPH" is used as a reference placeholder.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ShortText" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>"ShortText" is used to return miscellaneous frequent flyer-related information.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="Status" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>"Status" is used to return the status code associated with the particular flight segment within the record.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="Id" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>Reference ID</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="unbounded" name="Passport">
                <xs:annotation>
                  <xs:documentation>Passport details</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:attribute name="Number" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>"Number" is used to return the passenger's passport number.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="RPH" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>"RPH" is used as a reference placeholder.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="Id" type="xs:string">
                    <xs:annotation>
                      <xs:documentation>Reference ID</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="PaymentInfo">
                <xs:annotation>
                  <xs:documentation>Payment information</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element minOccurs="0" name="Payment">
                      <xs:annotation>
                        <xs:documentation>Payment Information</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element minOccurs="0" maxOccurs="unbounded" name="CC_Info">
                            <xs:annotation>
                              <xs:documentation>Credit card information</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element minOccurs="0" name="CardHolderInfo">
                                  <xs:annotation>
                                    <xs:documentation>Cardholder information</xs:documentation>
                                  </xs:annotation>
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element minOccurs="0" name="Address">
                                        <xs:annotation>
                                          <xs:documentation>Address</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                          <xs:sequence>
                                            <xs:element minOccurs="0" maxOccurs="4" name="AddressLine" type="xs:string">
                                              <xs:annotation>
                                                <xs:documentation>"AddressLine" is used to return the address associated with the particular credit card holder.</xs:documentation>
                                              </xs:annotation>
                                            </xs:element>
                                          </xs:sequence>
                                        </xs:complexType>
                                      </xs:element>
                                      <xs:element minOccurs="0" name="ContactNumbers">
                                        <xs:annotation>
                                          <xs:documentation>Contact numbers</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                          <xs:sequence>
                                            <xs:element minOccurs="0" name="ContactNumber">
                                              <xs:annotation>
                                                <xs:documentation>Contact number</xs:documentation>
                                              </xs:annotation>
                                              <xs:complexType>
                                                <xs:attribute name="Phone" type="xs:string">
                                                  <xs:annotation>
                                                    <xs:documentation>"Phone" is used to return the telephone number associated with the particular credit card holder.</xs:documentation>
                                                  </xs:annotation>
                                                </xs:attribute>
                                              </xs:complexType>
                                            </xs:element>
                                          </xs:sequence>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="Name" type="xs:string">
                                      <xs:annotation>
                                        <xs:documentation>"Name" is used to return the credit card holder's name.</xs:documentation>
                                      </xs:annotation>
                                    </xs:attribute>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                              <xs:attribute name="PreferredCustomer" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>"PreferredCustomer" is used to indicate whether or not the cardholder is a preferred customer.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="ShortText" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>"ShortText" is used to return miscellaneous credit card-related information.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                            </xs:complexType>
                          </xs:element>
                          <xs:element minOccurs="0" maxOccurs="unbounded" name="Form">
                            <xs:annotation>
                              <xs:documentation>Payment form</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element minOccurs="0" maxOccurs="unbounded" name="Text" type="xs:string">
                                  <xs:annotation>
                                    <xs:documentation>"Text" is used to return the form of payment.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                              <xs:attribute name="RPH" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>"RPH" is used as a reference place holder.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="Id" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>Reference ID</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" maxOccurs="unbounded" name="PersonName">
                <xs:annotation>
                  <xs:documentation>Person names</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element minOccurs="0" maxOccurs="unbounded" name="Email">
                      <xs:annotation>
                        <xs:documentation>"Email" is used to return the passenger's email address.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:simpleContent>
