      <xs:element minOccurs="0" maxOccurs="unbounded" name="NegotiatedFareCode" type="NegotiatedFareCodeType" />
      <xs:element minOccurs="0" maxOccurs="unbounded" name="AccountCode" type="AccountCodeType" />
      <xs:element minOccurs="0" name="PublicFare" type="IndDefaultFalseType">
        <xs:annotation>
          <xs:documentation xml:lang="en">This element finds only public fares.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="PrivateFare" type="IndDefaultFalseType">
        <xs:annotation>
          <xs:documentation xml:lang="en">This element finds only private fares.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="IATAFare" type="IndDefaultFalseType">
        <xs:annotation>
          <xs:documentation xml:lang="en">This element finds only IATA fares.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="WebFare" type="IndType">
        <xs:annotation>
          <xs:documentation xml:lang="en">Web fare</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="Priority" type="PriorityComplexType">
        <xs:annotation>
          <xs:documentation xml:lang="en">This element governs how flights are returned.
