          <xs:documentation xml:lang="en">Residency.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="E">
        <xs:annotation>
          <xs:documentation xml:lang="en">Employment.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="N">
        <xs:annotation>
          <xs:documentation xml:lang="en">Nationality.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="PassengerStatusType">
    <xs:sequence>
      <xs:element minOccurs="0" name="StateCode" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">CountryCode and CityCode are mutually exclusive, with StateCode being optional when CountryCode is present in original TIR schema,
                        this was simplified for easier xsd -&gt; JSON conversion.
                        For original schemas please refer to https://beta.developer.sabre.com/docs/soap_apis/management/itinerary/Get_Itinerary</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="CountryCode" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="en">CountryCode and CityCode are mutually exclusive, with StateCode being optional when CountryCode is present in original TIR schema,
                        this was simplified for easier xsd -&gt; JSON conversion.
                        For original schemas please refer to https://beta.developer.sabre.com/docs/soap_apis/management/itinerary/Get_Itinerary</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="CityCode" type="AlphaLength3">
        <xs:annotation>
          <xs:documentation xml:lang="en">CountryCode and CityCode are mutually exclusive, with StateCode being optional when CountryCode is present in original TIR schema,
                        this was simplified for easier xsd -&gt; JSON conversion.
                        For original schemas please refer to https://beta.developer.sabre.com/docs/soap_apis/management/itinerary/Get_Itinerary</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="type" type="PassengerStatusEnumType" use="required" />
  </xs:complexType>
  <xs:complexType name="ETicketableOverrideType">
    <xs:attribute name="value" type="xs:boolean">
      <xs:annotation>
        <xs:documentation xml:lang="en">ETicketable override</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="CurrencyType">
