                              <xs:attribute name="Type">
                                <xs:annotation>
                                  <xs:documentation>''Type'' returns requestor identifier type.
                                                                        ''Type'' can be found in OTA Table UIT : value 5 for travel agency.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                  <xs:restriction base="xs:int">
                                    <xs:minInclusive value="1" />
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:attribute>
                              <xs:attribute name="Id" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>''Id'' returns the agency IATA number.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="IdContext" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>''IdContext'' returns the agency IATA identifier context.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                            </xs:complexType>
                          </xs:element>
                          <xs:element minOccurs="0" name="AgencyAddress">
                            <xs:annotation>
                              <xs:documentation>''AgencyAddress'' returns travel agency address information.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="AddressLine1" type="xs:string">
                                  <xs:annotation>
                                    <xs:documentation>''AddressLine1'' returns the first agency address line.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element minOccurs="0" name="AddressLine2" type="xs:string">
                                  <xs:annotation>
                                    <xs:documentation>''AddressLine2'' returns the second agency address line.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element minOccurs="0" name="AddressLine3" type="xs:string">
                                  <xs:annotation>
                                    <xs:documentation>''AddressLine3'' returns the third agency address line.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element minOccurs="0" name="CityName">
                                  <xs:annotation>
                                    <xs:documentation>''CityName'' returns the agency city name.</xs:documentation>
                                  </xs:annotation>
                                  <xs:complexType>
                                    <xs:simpleContent>
                                      <xs:extension base="xs:string">
                                        <xs:attribute name="CityCode" type="xs:string">
                                          <xs:annotation>
                                            <xs:documentation>''CityCode'' returns the agency city code.</xs:documentation>
                                          </xs:annotation>
                                        </xs:attribute>
                                      </xs:extension>
                                    </xs:simpleContent>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element minOccurs="0" name="StateProv">
                                  <xs:annotation>
                                    <xs:documentation>''StateProv'' returns the agency state or province name.</xs:documentation>
                                  </xs:annotation>
                                  <xs:complexType>
                                    <xs:simpleContent>
                                      <xs:extension base="xs:string">
                                        <xs:attribute name="StateCode" type="xs:string">
                                          <xs:annotation>
                                            <xs:documentation>''StateCode'' returns the agency state code.</xs:documentation>
                                          </xs:annotation>
                                        </xs:attribute>
                                      </xs:extension>
                                    </xs:simpleContent>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element minOccurs="0" name="PostalCode" type="xs:string">
                                  <xs:annotation>
                                    <xs:documentation>''PostalCode'' returns the agency postal code.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="CountryName">
                                  <xs:annotation>
                                    <xs:documentation>''CountryName'' returns the agency country name.</xs:documentation>
                                  </xs:annotation>
                                  <xs:complexType>
                                    <xs:simpleContent>
                                      <xs:extension base="xs:string">
                                        <xs:attribute name="Code" use="required">
                                          <xs:annotation>
                                            <xs:documentation>''Code'' returns the agency country code.</xs:documentation>
                                          </xs:annotation>
                                          <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                              <xs:minLength value="2" />
                                              <xs:maxLength value="2" />
                                            </xs:restriction>
                                          </xs:simpleType>
                                        </xs:attribute>
                                      </xs:extension>
                                    </xs:simpleContent>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element minOccurs="0" name="AgencyContact">
                            <xs:annotation>
                              <xs:documentation>''AgencyContact'' returns the travel agency contact information.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:attribute name="Phone" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>''Phone'' returns the agency phone number.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="Mobile" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>''Mobile'' returns the agency mobile phone number.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="Fax" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>''Fax'' returns the agency fax number.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                        <xs:attribute name="AgencyName" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''AgencyName'' returns the travel agency name.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="AgentSine" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''AgentSine'' returns the travel agent sabre system sine.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="AgentDutyCode" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''AgentDutyCode'' returns the travel agent sabre system duty code.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Lniata" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''Lniata'' returns the sabre system session LNIATA.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="AgentOriginCity" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''AgentOriginCity'' returns the sabre system agent origin pseudo city code.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="ISOCountryCode" type="xs:string" use="required">
                          <xs:annotation>
                            <xs:documentation>''ISOCountryCode'' returns the travel agency sabre system ISO country code.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="ISOCurrencyCode" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''ISOCurrencyCode'' returns the travel agency sabre system ISO currency code.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="AuxPlaceOfOrigin" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''AuxPlaceOfOrigin'' returns the auxiliary place of origin.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="IATAOriginatorTypeCode" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''IATAOriginatorTypeCode'' returns the IATA originator type code.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CreationDate" type="xs:date">
                          <xs:annotation>
                            <xs:documentation>''CreationDate'' returns the lodging booking creation date.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CreationTime" type="xs:time">
                          <xs:annotation>
                            <xs:documentation>''CreationTime'' returns the lodging booking creation time.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="PseudoCityCode" type="xs:string" use="required">
                          <xs:annotation>
                            <xs:documentation>''PseudoCityCode'' returns the sabre system agent pseudo city code.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="ValueAdds">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element maxOccurs="unbounded" name="ValueAdd">
                      <xs:annotation>
                        <xs:documentation>''ValueAdds'' returns value adds returned for EAN booking.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element minOccurs="0" maxOccurs="unbounded" name="Text">
                            <xs:annotation>
                              <xs:documentation>''Text'' returns value add text.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="VoucherInformation">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element minOccurs="0" name="VoucherAttributes">
                      <xs:annotation>
                        <xs:documentation>''VoucherAttributes'' returns voucher attributes.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element maxOccurs="unbounded" name="VoucherAttribute">
                            <xs:annotation>
                              <xs:documentation>''VoucherAttribute'' returns Type,Value pairs.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:attribute name="Type" type="xs:string" use="required">
                                <xs:annotation>
                                  <xs:documentation>''Type'' returns the voucher attribute type.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="Value" type="xs:string" use="required">
                                <xs:annotation>
