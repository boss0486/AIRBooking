    <xs:attribute name="tType" type="xs:boolean">
      <xs:annotation>
        <xs:documentation>Indicator Returning T-Type OB Fees</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name="FareAdjustmentType">
    <xs:attribute name="value" type="xs:string" use="required">
