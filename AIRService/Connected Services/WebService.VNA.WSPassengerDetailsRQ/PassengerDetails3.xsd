                                    <xs:sequence>
                                      <xs:element minOccurs="0" maxOccurs="unbounded" name="Text" type="xs:string">
                                        <xs:annotation>
                                          <xs:documentation>''Text'' returns the product description text.</xs:documentation>
                                        </xs:annotation>
