                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="Rooms">
                <xs:annotation>
                  <xs:documentation>''Rooms'' returns a list of rooms.</xs:documentation>
                </xs:annotation>
