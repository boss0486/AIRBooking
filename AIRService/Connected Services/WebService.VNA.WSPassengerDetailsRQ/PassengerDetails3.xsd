    <xs:attribute name="returnBrandAncillaries" type="xs:boolean">
      <xs:annotation>
        <xs:documentation xml:lang="en">Return ancillaries from the ATPCO Branded Fares Feature Table 166</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:simpleType name="PassengerStatusEnumType">
