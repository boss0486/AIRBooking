                  <xs:sequence>
                    <xs:element maxOccurs="unbounded" name="Room">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element minOccurs="0" name="Guests">
                            <xs:annotation>
                              <xs:documentation>''Guests'' returns a list of Guests in a room.</xs:documentation>
                            </xs:annotation>
