                                        <xs:restriction base="xs:int">
                                          <xs:minInclusive value="1" />
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:attribute>
                                    <xs:attribute name="Index">
                                      <xs:annotation>
                                        <xs:documentation>''Index'' returns guest index.</xs:documentation>
                                      </xs:annotation>
                                      <xs:simpleType>
                                        <xs:restriction base="xs:int">
                                          <xs:minInclusive value="1" />
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:attribute>
                                    <xs:attribute default="false" name="LeadGuest" type="xs:boolean">
                                      <xs:annotation>
                                        <xs:documentation>''LeadGuest'' returns the lead guest indicator.
                                                                                    Only one lead passenger can be present per booking.</xs:documentation>
                                      </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="FrequentFlyerNumber">
                                      <xs:annotation>
                                        <xs:documentation>''FrequentFlyerNumber'' returns the frequent flyer number.</xs:documentation>
                                      </xs:annotation>
                                      <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                          <xs:minLength value="1" />
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:attribute>
                                    <xs:attribute name="LoyaltyId">
                                      <xs:annotation>
                                        <xs:documentation>''LoyaltyId'' returns the loyalty identifier.</xs:documentation>
                                      </xs:annotation>
                                      <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                          <xs:minLength value="1" />
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:attribute>
                                    <xs:attribute name="Title" type="xs:string">
                                      <xs:annotation>
                                        <xs:documentation>''Title'' returns guest title.
                                                                                    Title can include MR./MS. etc.</xs:documentation>
                                      </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="FirstName" type="xs:string">
                                      <xs:annotation>
                                        <xs:documentation>''FirstName'' returns the first name of the guest.</xs:documentation>
                                      </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="LastName" type="xs:string">
                                      <xs:annotation>
                                        <xs:documentation>''LastName'' returns the last name of the guest.</xs:documentation>
                                      </xs:annotation>
                                    </xs:attribute>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                              <xs:attribute name="Count" type="xs:int">
                                <xs:annotation>
                                  <xs:documentation>''Count'' returns the guest count.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                            </xs:complexType>
                          </xs:element>
                          <xs:element minOccurs="0" name="RoomDescription">
                            <xs:annotation>
                              <xs:documentation>''RoomDescription'' returns supplier provided room description.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element minOccurs="0" name="Text" type="xs:string">
                                  <xs:annotation>
                                    <xs:documentation>''Text'' returns supplier provided room description text.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                              <xs:attribute name="Name" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>''Name'' returns supplier provided text.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                            </xs:complexType>
                          </xs:element>
                          <xs:element minOccurs="0" name="RoomExtras">
                            <xs:annotation>
                              <xs:documentation>''RoomExtras'' returns a list of room extras.
                                                                Only 3 supported crib/rollaway bed/extra per person.</xs:documentation>
