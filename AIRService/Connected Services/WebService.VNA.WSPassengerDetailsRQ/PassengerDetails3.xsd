                              <xs:sequence>
                                <xs:element maxOccurs="unbounded" name="Guest">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element minOccurs="0" name="Contact">
                                        <xs:annotation>
                                          <xs:documentation>''Contact'' returns guest contact information.</xs:documentation>
                                        </xs:annotation>
                                        <xs:complexType>
                                          <xs:attribute name="Phone" type="xs:string">
                                            <xs:annotation>
                                              <xs:documentation>''Phone'' returns the guest contact phone number.</xs:documentation>
                                            </xs:annotation>
                                          </xs:attribute>
                                          <xs:attribute name="Mobile" type="xs:string">
                                            <xs:annotation>
                                              <xs:documentation>''Mobile'' returns the guest contact mobile phone number.</xs:documentation>
                                            </xs:annotation>
                                          </xs:attribute>
                                          <xs:attribute name="Fax" type="xs:string">
                                            <xs:annotation>
                                              <xs:documentation>''Fax'' returns the guest contact fax number.</xs:documentation>
                                            </xs:annotation>
                                          </xs:attribute>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="Type">
                                      <xs:annotation>
                                        <xs:documentation>''Type'' returns the guest type.
                                                                                    AQC OTA Code table is used for Type.
                                                                                    `10` - Adult.
                                                                                    `8` - Child.
                                                                                    Valid values are only Adult and Child Types.</xs:documentation>
                                      </xs:annotation>
                                      <xs:simpleType>
                                        <xs:restriction base="xs:int">
                                          <xs:minInclusive value="1" />
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:attribute>
                                    <xs:attribute name="FullName" type="xs:string">
                                      <xs:annotation>
                                        <xs:documentation>''FullName'' is returned sometimes by aggregator when it does not return first name / last name.</xs:documentation>
                                      </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="Email">
                                      <xs:annotation>
                                        <xs:documentation>''Email'' returns email address.</xs:documentation>
                                      </xs:annotation>
                                      <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                          <xs:pattern value="[A-Z0-9a-z_.]*@[A-Z0-9a-z_.]*" />
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:attribute>
                                    <xs:attribute name="Age">
                                      <xs:annotation>
                                        <xs:documentation>''Age'' returns guest age.
                                                                                    Age is mandatory for a child.</xs:documentation>
