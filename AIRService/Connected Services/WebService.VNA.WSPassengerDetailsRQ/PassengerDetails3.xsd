                                        <xs:documentation>''LocalFeesInclusive'' returns the local fees inclusive indicator.</xs:documentation>
                                      </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="IncidentalsInclusive" type="xs:boolean">
                                      <xs:annotation>
                                        <xs:documentation>''IncidentalsInclusive'' returns the incidentals inclusive indicator.</xs:documentation>
                                      </xs:annotation>
                                    </xs:attribute>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                              <xs:attribute name="RatePlanName" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>''RatePlanName'' returns the rate plan name.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="RatePlanCode" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>''RatePlanCode'' returns the client ID code (HIC table in PSS).</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="SupplierRatePlanCode" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>''SupplierRatePlanCode'' returns actual rate code (HST transaction in PSS).</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="RatePlanType" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>''RatePlanType'' returns the rate plan type.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="PrepaidIndicator" type="xs:boolean">
                                <xs:annotation>
                                  <xs:documentation>''PrepaidIndicator'' returns the prepaid indicator.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="RateAssured" type="xs:string">
                                <xs:annotation>
                                  <xs:documentation>''RateAssured'' returns the rate assured information.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="RateKey">
                                <xs:annotation>
                                  <xs:documentation>''RateKey'' returns the rate key.</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                  <xs:restriction base="xs:string">
                                    <xs:minLength value="1" />
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:attribute>
                            </xs:complexType>
                          </xs:element>
                          <xs:element minOccurs="0" name="ValueAdds">
                            <xs:annotation>
                              <xs:documentation>''ValueAdds'' returns value adds returned for EAN booking.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element maxOccurs="unbounded" name="ValueAdd">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element minOccurs="0" maxOccurs="unbounded" name="Text">
                                        <xs:annotation>
                                          <xs:documentation>''Text'' returns value add text.</xs:documentation>
                                        </xs:annotation>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element minOccurs="0" name="SpecialInstructions">
                            <xs:annotation>
                              <xs:documentation>''SpecialInstructions'' returns free form or GTA special instructions.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element maxOccurs="unbounded" name="SpecialInstruction">
                                  <xs:annotation>
                                    <xs:documentation>''SpecialInstruction'' returns special instruction.</xs:documentation>
                                  </xs:annotation>
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string" />
                                  </xs:simpleType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                          <xs:element minOccurs="0" name="SearchParameters">
                            <xs:annotation>
                              <xs:documentation>''SearchParameters'' returns search parameters.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element maxOccurs="unbounded" name="SearchParameter">
                                  <xs:annotation>
                                    <xs:documentation>''SearchParameter'' returns Type,Value pairs.</xs:documentation>
                                  </xs:annotation>
                                  <xs:complexType>
                                    <xs:attribute name="Type" type="xs:string" use="required">
                                      <xs:annotation>
                                        <xs:documentation>''Type'' returns the search parameter type.</xs:documentation>
                                      </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="Value" type="xs:string" use="required">
                                      <xs:annotation>
                                        <xs:documentation>''Value'' returns the search parameter value.</xs:documentation>
                                      </xs:annotation>
                                    </xs:attribute>
                                  </xs:complexType>
                                </xs:element>
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                        <xs:attribute name="RoomTypeCode" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''RoomTypeCode'' returns a specific system room type code, ex: A1K, A1Q etc.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="RoomType" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''RoomType'' returns a code value that indicates the type of room, e.g.: double, king, etc.
                                                            Values may use the Hotel Descriptive Content table or codes specific to the property or hotel brand.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="InvBlockCode" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''InvBlockCode'' returns a RoomType code equivalent for GDS suppliers response.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="RoomCategory" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''RoomCategory'' returns the room category.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="RoomId" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''RoomId'' returns the room identifier.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="SegmentNumber" type="xs:int">
                          <xs:annotation>
                            <xs:documentation>''SegmentNumber'' returns the segment number.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Floor" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''Floor'' returns the floor.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="RoomViewCode" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''RoomViewCode'' returns the room view code.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="SharedRoomInd" type="xs:boolean">
                          <xs:annotation>
                            <xs:documentation>''SharedRoomInd'' returns the shared room indicator.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="GuestRoomInfo">
                          <xs:annotation>
                            <xs:documentation>''GuestRoomInfo'' returns the number of guests in a room.</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:int">
                              <xs:minInclusive value="1" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="Action">
                          <xs:annotation>
                            <xs:documentation>''Action'' returns the Action Code For Update.</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:enumeration value="CREATE" />
                              <xs:enumeration value="CANCEL" />
                              <xs:enumeration value="MODIFY" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="NonSmoking" type="xs:boolean">
                          <xs:annotation>
                            <xs:documentation>''NonSmoking'' returns the non smoking indicator.
                                                            `True` - Non Smoking.
                                                            `False` - Smoking.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="BedTypeCode">
                          <xs:annotation>
                            <xs:documentation>''BedTypeCode'' indicates the type of bed(s) found in the room.
                                                            Typical values would be Double, Twin, Queen, or King.
                                                            Refer to OpenTravel Code List Bed Type Code (BED).</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:int">
                              <xs:minInclusive value="1" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="AccessibilityInformation">
                          <xs:annotation>
                            <xs:documentation>''AccessibilityInformation'' returns OTA Types Code, Uses PHY table.</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:int">
                              <xs:minInclusive value="1" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="RoomIndex">
                          <xs:annotation>
                            <xs:documentation>''RoomIndex'' returns the sequence number of the room.</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:int">
                              <xs:minInclusive value="1" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="BedTypeDescription" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''BedTypeDescription'' is Used for NON OTA Bed Type.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="NumberOfRooms" type="xs:int">
                    <xs:annotation>
                      <xs:documentation>''NumberOfRooms'' returns the number of rooms.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="GuestCount" type="xs:int">
                    <xs:annotation>
                      <xs:documentation>''GuestCount'' returns the count of guests.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="SpecialInstructions">
                <xs:annotation>
                  <xs:documentation>''SpecialInstructions'' returns free form or GTA special instructions.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element maxOccurs="unbounded" name="SpecialInstruction">
                      <xs:annotation>
                        <xs:documentation>''SpecialInstruction'' returns special instruction.</xs:documentation>
                      </xs:annotation>
                      <xs:simpleType>
                        <xs:restriction base="xs:string" />
                      </xs:simpleType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="PaymentInformation">
                <xs:annotation>
                  <xs:documentation>''PaymentInformation'' returns payment information.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="FormOfPaymentReference">
                      <xs:annotation>
                        <xs:documentation>''FormOfPaymentReference'' returns a reference to a form of payment.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:attribute name="ref" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''ref'' returns the form of payment reference.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="newRef" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''newRef'' returns the new form of payment reference.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="deploymentId" type="xs:string">
                          <xs:annotation>
                            <xs:documentation>''deploymentId'' returns the deployment identifier.</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="Type">
                    <xs:annotation>
                      <xs:documentation>''Type'' returns the form of payment type.
                                                ''Type'' can be DEPOSIT or GUARANTEE.</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:enumeration value="DEPOSIT" />
                        <xs:enumeration value="GUARANTEE" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="ArrivalDepartureInformation">
                <xs:annotation>
                  <xs:documentation>''ArrivalDepartureInformation'' returns arrival/departure information.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element minOccurs="0" name="ArrivalFlight">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Carrier">
                            <xs:annotation>
                              <xs:documentation>''Carrier'' returns the arrival flight carrier code.</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:length value="2" />
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="FlightNumber">
                            <xs:annotation>
                              <xs:documentation>''FlightNumber'' returns the arrival flight number.</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:length value="5" />
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="DepartureFlight">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Carrier">
                            <xs:annotation>
                              <xs:documentation>''Carrier'' returns the departure flight carrier code.</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:length value="2" />
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="FlightNumber">
                            <xs:annotation>
                              <xs:documentation>''FlightNumber'' returns the departure flight number.</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:length value="5" />
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="ArrivalTime" type="xs:time">
                    <xs:annotation>
                      <xs:documentation>''ArrivalTime'' returns the arrival time.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="DepartureTime" type="xs:time">
                    <xs:annotation>
                      <xs:documentation>''DepartureTime'' returns the departure time.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="POS">
                <xs:annotation>
                  <xs:documentation>''POS'' returns point of sale information.</xs:documentation>
