                            <attribute name="PseudoCityCode" type="string">
                              <annotation>
                                <documentation>Used to specify a pseudo city code.</documentation>
                              </annotation>
                            </attribute>
                            <attribute name="QueueNumber" type="integer">
                              <annotation>
                                <documentation>Used to specify a queue number.</documentation>
                              </annotation>
                            </attribute>
                            <attribute name="QueueName" type="string">
                              <annotation>
                                <documentation>Used to specify a special name queue.</documentation>
                              </annotation>
                            </attribute>
                            <attribute name="ShortText" type="string">
                              <annotation>
                                <documentation>Used to specify miscellaneous free text.</documentation>
                              </annotation>
