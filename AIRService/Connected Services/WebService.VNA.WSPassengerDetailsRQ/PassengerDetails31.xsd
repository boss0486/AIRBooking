<?xml version="1.0" encoding="utf-8"?>
<schema xmlns:spct="http://services.sabre.com/sp/common/simple/v3" xmlns:stlp="http://services.sabre.com/STL_Payload/v02_01" elementFormDefault="qualified" targetNamespace="http://services.sabre.com/sp/pd/v3_4" xmlns="http://www.w3.org/2001/XMLSchema">
  <import schemaLocation="built-ins/STL2_Payload_v02_01.xsd" namespace="http://services.sabre.com/STL_Payload/v02_01" />
  <import schemaLocation="built-ins/SpCommonSimpleTypes_v3.xsd" namespace="http://services.sabre.com/sp/common/simple/v3" />
  <element name="PassengerDetailsRQ">
    <complexType>
      <complexContent mixed="false">
        <extension base="stlp:STL_Request_Payload">
          <sequence>
            <element minOccurs="0" name="MiscSegmentSellRQ">
              <complexType>
                <sequence>
                  <element name="MiscSegment">
                    <annotation>
                      <documentation>Used to book miscellaneous segments such as Miscellaneous Charge Orders, Insurance Segments, Other product segments.</documentation>
                    </annotation>
                    <complexType>
                      <sequence>
                        <element name="OriginLocation">
                          <annotation>
                            <documentation>Used to specify the location to be associated with the miscellaneous segment.</documentation>
                          </annotation>
                          <complexType>
                            <attribute name="LocationCode" type="string" use="required">
                              <annotation>
                                <documentation>Used to specify the location code to be associated with the miscellaneous segment.</documentation>
                              </annotation>
                            </attribute>
                          </complexType>
                        </element>
                        <element name="Text" type="string">
                          <annotation>
                            <documentation>Used to specify free text information to be associated with the miscellaneous segment.</documentation>
                          </annotation>
                        </element>
                        <element name="VendorPrefs">
                          <annotation>
                            <documentation>Used to specify a particular vendor code.</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element name="Airline">
                                <annotation>
                                  <documentation>Used to specify a particular airline.</documentation>
                                </annotation>
                                <complexType>
                                  <attribute name="Code" type="string" use="required">
                                    <annotation>
                                      <documentation>Used to specify a particular vendor code.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                            </sequence>
                          </complexType>
                        </element>
                      </sequence>
                      <attribute name="DepartureDateTime" type="spct:date" use="required">
                        <annotation>
                          <documentation>Used to specify the date associated with the miscellaneous segment.</documentation>
                        </annotation>
                      </attribute>
                      <attribute name="InsertAfter" type="integer">
                        <annotation>
                          <documentation>Used to denote a segment after which the miscellaneous segment needs to be placed.</documentation>
                        </annotation>
                      </attribute>
                      <attribute name="NumberInParty" type="integer" use="required">
                        <annotation>
                          <documentation>Used to specify the number of parties associated with the miscellaneous segment.</documentation>
                        </annotation>
                      </attribute>
                      <attribute name="Status" type="string" use="required">
                        <annotation>
                          <documentation>Used to specify the action code to be used to sell the segment. Example: "GK".</documentation>
                        </annotation>
                      </attribute>
                      <attribute name="Type" use="required">
                        <annotation>
                          <documentation>Used to specify the type of miscellaneous segment, i.e. OTH, MCO, PTA, or INS.</documentation>
                        </annotation>
                        <simpleType>
                          <restriction base="string">
                            <enumeration value="INS" />
                            <enumeration value="MCO" />
                            <enumeration value="OTH" />
                            <enumeration value="PTA" />
                          </restriction>
                        </simpleType>
                      </attribute>
                    </complexType>
                  </element>
                </sequence>
              </complexType>
            </element>
            <element minOccurs="0" name="PostProcessing">
              <annotation>
                <documentation>Used to specify additional variables used to commit changes to or create a passenger name record.</documentation>
              </annotation>
              <complexType>
                <sequence>
                  <element minOccurs="0" name="RedisplayReservation">
                    <annotation>
                      <documentation>Used to indicate whether or not to redisplay the PNR.</documentation>
                    </annotation>
                    <complexType>
                      <attribute name="waitInterval">
                        <annotation>
                          <documentation>Used  to indicate the wait interval in milliseconds that the system should wait before redisplaying the reservation.</documentation>
                        </annotation>
                        <simpleType>
                          <restriction base="integer">
                            <minInclusive value="100" />
                            <maxInclusive value="10000" />
                          </restriction>
                        </simpleType>
                      </attribute>
                    </complexType>
                  </element>
                  <element minOccurs="0" name="ARUNK_RQ">
                    <annotation>
                      <documentation>Used to add arrival unknown segments into a passenger name record.
                                                The system will properly apply them so there is no need to segment select, etc... .</documentation>
                    </annotation>
                  </element>
                  <element minOccurs="0" name="EndTransactionRQ">
                    <complexType>
                      <sequence>
                        <element minOccurs="0" name="EndTransaction">
                          <annotation>
                            <documentation>Used to end the transaction and finalize the record.</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element minOccurs="0" name="Email">
                                <annotation>
                                  <documentation>Used to send an email notification upon end transaction to any email addresses contained within the particular record.</documentation>
                                </annotation>
                                <complexType>
                                  <sequence>
                                    <element minOccurs="0" name="eTicket">
                                      <annotation>
                                        <documentation>Used to send an email notification containing a text-based copy of the eTicket.</documentation>
                                      </annotation>
                                      <complexType>
                                        <sequence>
                                          <element minOccurs="0" name="PDF">
                                            <annotation>
                                              <documentation>Used to send an email notification containing a PDF-based copy of the eTicket.</documentation>
                                            </annotation>
                                            <complexType>
                                              <attribute name="Ind" type="boolean" use="required">
                                                <annotation>
                                                  <documentation>Enables the functionality when set to `true`.</documentation>
                                                </annotation>
                                              </attribute>
                                            </complexType>
                                          </element>
                                        </sequence>
                                        <attribute name="Ind" type="boolean" use="required">
                                          <annotation>
                                            <documentation>Enables the functionality when set to `true`.</documentation>
                                          </annotation>
                                        </attribute>
                                      </complexType>
                                    </element>
                                    <element minOccurs="0" name="Invoice">
                                      <annotation>
                                        <documentation>Used to send an email notification containing a text-based copy of the invoice.</documentation>
                                      </annotation>
                                      <complexType>
                                        <attribute name="Ind" type="boolean" use="required">
                                          <annotation>
                                            <documentation>Enables the functionality when set to `true`.</documentation>
                                          </annotation>
                                        </attribute>
                                      </complexType>
                                    </element>
                                    <element minOccurs="0" name="Itinerary">
                                      <annotation>
                                        <documentation>Used to send an email notification containing a text-based copy of the itinerary.</documentation>
                                      </annotation>
                                      <complexType>
                                        <sequence>
                                          <element minOccurs="0" name="PDF">
                                            <annotation>
                                              <documentation>Used to send an email notification containing a PDF-based copy of the itinerary.</documentation>
                                            </annotation>
                                            <complexType>
                                              <attribute name="Ind" type="boolean" use="required">
                                                <annotation>
                                                  <documentation>Enables the functionality when set to `true`.</documentation>
                                                </annotation>
                                              </attribute>
                                            </complexType>
                                          </element>
                                          <element minOccurs="0" maxOccurs="unbounded" name="Segment">
                                            <complexType>
                                              <attribute name="EndNumber" type="integer">
                                                <annotation>
                                                  <documentation>Used to specify an ending segment in the range to include in the email message.</documentation>
                                                </annotation>
                                              </attribute>
                                              <attribute name="Number" type="integer" use="required">
                                                <annotation>
                                                  <documentation>Used to specify a segment number to include in the email message.</documentation>
                                                </annotation>
                                              </attribute>
                                            </complexType>
                                          </element>
                                        </sequence>
                                        <attribute name="Ind" type="boolean" use="required">
                                          <annotation>
                                            <documentation>Enables the functionality when set to `true`.</documentation>
                                          </annotation>
                                        </attribute>
                                      </complexType>
                                    </element>
                                    <element minOccurs="0" name="PersonName">
                                      <annotation>
                                        <documentation>Used to specify a particular passenger name to send the email notification to.</documentation>
                                      </annotation>
                                      <complexType>
                                        <attribute name="NameNumber" type="string" use="required">
                                          <annotation>
                                            <documentation>Used to specify a particular passenger name number to send the email notification to.
                                                                                            Please note that the email address in the PNR`s email field must be name associated to the specified name number.</documentation>
                                          </annotation>
                                        </attribute>
                                      </complexType>
                                    </element>
                                  </sequence>
                                  <attribute name="Ind" type="boolean" use="required">
                                    <annotation>
                                      <documentation>Enables the functionality when set to `true`.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                              <element minOccurs="0" name="ScheduleChange">
                                <annotation>
                                  <documentation>Used to update schedule changes.</documentation>
                                </annotation>
                                <complexType>
                                  <attribute name="Ind" type="boolean" use="required">
                                    <annotation>
                                      <documentation>The schedule change indicator.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                            </sequence>
                            <attribute name="Ind" type="boolean" use="required">
                              <annotation>
                                <documentation>Indicates whether the `E` host command should be sent or only the `6` (Source) host command.</documentation>
                              </annotation>
                            </attribute>
                          </complexType>
                        </element>
                        <element minOccurs="0" name="Source">
                          <annotation>
                            <documentation>Used to identify the entity that authorized the changes in a Passenger Name Record.</documentation>
                          </annotation>
                          <complexType>
                            <attribute name="ReceivedFrom" type="string" use="required">
                              <annotation>
                                <documentation>Used to pass the source name.</documentation>
                              </annotation>
                            </attribute>
                          </complexType>
                        </element>
                      </sequence>
                    </complexType>
                  </element>
                  <element minOccurs="0" name="QueuePlaceRQ">
                    <annotation>
                      <documentation>Used to request to place the newly created PNR on a queue.</documentation>
                    </annotation>
                    <complexType>
                      <sequence>
                        <element minOccurs="0" name="MultiQueuePlace">
                          <annotation>
                            <documentation>Used to request to place the newly created PNR on multiple queues.</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element maxOccurs="20" name="QueueIdentifier">
                                <annotation>
                                  <documentation>Used to pass a single queue details.</documentation>
                                </annotation>
                                <complexType>
                                  <attribute name="Number" type="string" use="required">
                                    <annotation>
                                      <documentation>Used to pass the queue number.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="PrefatoryInstructionCode" type="string" use="required">
                                    <annotation>
                                      <documentation>Used to pass the prefatory instruction code (PIC).</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="PseudoCityCode" type="string" use="required">
                                    <annotation>
                                      <documentation>Used to pass the pseudo city code (PCC).</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                            </sequence>
                          </complexType>
                        </element>
                        <element minOccurs="0" name="QueueInfo">
                          <annotation>
                            <documentation>Used to request to place the newly created PNR on a single queue.</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element maxOccurs="3" name="QueueIdentifier">
                                <annotation>
                                  <documentation>Used to pass a single queue details.</documentation>
                                </annotation>
                                <complexType>
                                  <attribute name="Name" type="string">
                                    <annotation>
                                      <documentation>Used to specify the queue name where the record is to be placed.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="Number" type="string">
                                    <annotation>
                                      <documentation>Used to specify the queue number where the record is to be placed.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="PrefatoryInstructionCode" type="string">
                                    <annotation>
                                      <documentation>Used to specify a prefatory instruction code.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="PseudoCityCode" type="string">
                                    <annotation>
                                      <documentation>Used to specify a pseudo city code.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                              <element minOccurs="0" maxOccurs="6" name="UniqueID">
                                <complexType>
                                  <attribute name="ID" type="string" use="required">
                                    <annotation>
                                      <documentation>Used to specify a record locator to be queue placed.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                            </sequence>
                          </complexType>
                        </element>
                      </sequence>
                    </complexType>
                  </element>
                </sequence>
                <attribute name="ignoreAfter" type="boolean">
                  <annotation>
                    <documentation>Used to perform an ignore to clear out the AAA.
                                            Please note that it does not close the session.</documentation>
                  </annotation>
                </attribute>
                <attribute default="false" name="unmaskCreditCard" type="boolean">
                  <annotation>
                    <documentation>Used to unmask credit card information in the TIR response.
                                            Please note that this attribute has effect only if a user has EPR keyword CCVIEW.</documentation>
                  </annotation>
                </attribute>
              </complexType>
            </element>
            <element minOccurs="0" name="PreProcessing">
              <complexType>
                <sequence>
                  <element minOccurs="0" name="UniqueID">
                    <complexType>
                      <attribute name="id" type="string" use="required">
                        <annotation>
                          <documentation>Used to specify a record locator.</documentation>
                        </annotation>
                      </attribute>
                    </complexType>
                  </element>
                </sequence>
                <attribute name="ignoreBefore" type="boolean">
                  <annotation>
                    <documentation>Used to perform an ignore to clear out the AAA.
                                            Please note that it does not close the session.</documentation>
                  </annotation>
                </attribute>
              </complexType>
            </element>
            <element minOccurs="0" name="PriceQuoteInfo">
              <annotation>
                <documentation>Used to associate price quote records stored as a result of a previous Enhanced_AirBookRQ, or Enhanced_AirBookWithTaxRQ with passenger names in the PassengerDetailsRQ.</documentation>
              </annotation>
              <complexType>
                <sequence>
                  <element maxOccurs="98" name="Link">
                    <complexType>
                      <attribute name="hostedCarrier" type="boolean">
                        <annotation>
                          <documentation>Used to tell the system that the request is coming from a hosted carrier, since the method of linking price quote records to names is slightly different between SabreSonic-hosted carriers, and Sabre Travel Network-based users.</documentation>
                        </annotation>
                      </attribute>
                      <attribute name="nameNumber" type="string">
                        <annotation>
                          <documentation>Used to specify a passenger name number.
                                                        SabreSonic-hosted carriers can omit `NameNumber` if all passengers need to be associated with a single price quote record.
                                                        Sabre Travel Network-based users:
                                                        If you want to associate all passengers with a single price quote record omit the `NameNumber` attribute.</documentation>
                        </annotation>
                      </attribute>
                      <attribute name="record" type="string" use="required">
                        <annotation>
                          <documentation>Used to specify a price quote record.</documentation>
                        </annotation>
                      </attribute>
                    </complexType>
                  </element>
                </sequence>
              </complexType>
            </element>
            <element minOccurs="0" name="ProfileRQ">
              <annotation>
                <documentation>Used to add profile information.</documentation>
              </annotation>
              <complexType>
                <sequence>
                  <element name="UniqueID">
                    <annotation>
                      <documentation>Used to pass profile ID.</documentation>
                    </annotation>
                    <complexType>
                      <attribute name="id" type="string" use="required">
                        <annotation>
                          <documentation>Used to specify a profile name that is to be moved into the AAA to create the PNR.</documentation>
                        </annotation>
                      </attribute>
                    </complexType>
                  </element>
                </sequence>
              </complexType>
            </element>
            <element minOccurs="0" name="SpecialReqDetails">
              <annotation>
                <documentation>Used to add additional special requests such as seats, remarks, SSRs.</documentation>
              </annotation>
              <complexType>
                <sequence>
                  <element minOccurs="0" name="AddRemarkRQ">
                    <annotation>
                      <documentation>Used to add remarks.</documentation>
                    </annotation>
                    <complexType>
                      <sequence>
                        <element name="RemarkInfo">
                          <annotation>
                            <documentation>Used to add details of the remark.</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element minOccurs="0" name="FOP_Remark">
                                <annotation>
                                  <documentation>Used to add a form of payment remark.</documentation>
                                </annotation>
                                <complexType>
                                  <sequence>
                                    <element minOccurs="0" name="CC_Info">
                                      <annotation>
                                        <documentation>Used to add credit card details.</documentation>
                                      </annotation>
                                      <complexType>
                                        <sequence>
                                          <element name="PaymentCard">
                                            <annotation>
                                              <documentation>Used to add the card details.</documentation>
                                            </annotation>
                                            <complexType>
                                              <attribute name="AirlineCode" type="string">
                                                <annotation>
                                                  <documentation>Used to specify an airline code to be used in conjunction with a credit card security code.</documentation>
                                                </annotation>
                                              </attribute>
                                              <attribute name="CardSecurityCode" type="string">
                                                <annotation>
                                                  <documentation>Used to specify a credit card security code.</documentation>
                                                </annotation>
                                              </attribute>
                                              <attribute name="Code" type="string" use="required">
                                                <annotation>
                                                  <documentation>Used to specify a credit card vendor code.</documentation>
                                                </annotation>
                                              </attribute>
                                              <attribute name="ExpireDate" type="spct:expireDate" use="required">
                                                <annotation>
                                                  <documentation>Used to specify a credit card expiration date.</documentation>
                                                </annotation>
                                              </attribute>
                                              <attribute name="ExtendedPayment" type="string">
                                                <annotation>
                                                  <documentation>Used to define the number of months for extended payment.</documentation>
                                                </annotation>
                                              </attribute>
                                              <attribute name="ManualApprovalCode" type="string">
                                                <annotation>
                                                  <documentation>Used to pass a manual credit card approval code.</documentation>
                                                </annotation>
                                              </attribute>
                                              <attribute name="Number" type="string" use="required">
                                                <annotation>
                                                  <documentation>Used to specify a credit card number.</documentation>
                                                </annotation>
                                              </attribute>
                                              <attribute name="SuppressApprovalCode" type="boolean">
                                                <annotation>
                                                  <documentation>Used to specify to prevent the Sabre system from generating an approval code when the credit card number is input into the system.</documentation>
                                                </annotation>
                                              </attribute>
                                            </complexType>
                                          </element>
                                        </sequence>
                                        <attribute name="Suppress" type="boolean">
                                          <annotation>
                                            <documentation>Used to specify whether or not to suppress credit card information on an itinerary.</documentation>
                                          </annotation>
                                        </attribute>
                                      </complexType>
                                    </element>
                                  </sequence>
                                  <attribute name="Type" type="string">
                                    <annotation>
                                      <documentation>Used to specify the form of payment type.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                              <element minOccurs="0" name="FutureQueuePlaceRemark">
                                <annotation>
                                  <documentation>Used to send a future queue place remark.</documentation>
                                </annotation>
                                <complexType>
                                  <attribute name="Date" type="spct:date" use="required">
                                    <annotation>
                                      <documentation>Used to specify a queue placement date.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="PrefatoryInstructionCode" type="string" use="required">
                                    <annotation>
                                      <documentation>Used to specify the prefatory instruction code.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="PseudoCityCode" type="string">
                                    <annotation>
                                      <documentation>Used to specify a pseudo city code.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="QueueIdentifier" type="string" use="required">
                                    <annotation>
                                      <documentation>Used to specify a queue name/number.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="Time" type="spct:time">
                                    <annotation>
                                      <documentation>Used to specify a queue placement time.
                                                                                Please note that only whole hours can be specified, i.e. 12:00.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                              <element minOccurs="0" maxOccurs="98" name="Remark">
                                <annotation>
                                  <documentation>Used for multiple remark types.</documentation>
                                </annotation>
                                <complexType>
                                  <sequence>
                                    <element name="Text" type="string">
                                      <annotation>
                                        <documentation>Used to specify remark-related text.</documentation>
                                      </annotation>
                                    </element>
                                  </sequence>
                                  <attribute name="Code" type="string">
                                    <annotation>
                                      <documentation>Used to specify an alpha sorting character such as: `H`.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="SegmentNumber" type="string">
                                    <annotation>
                                      <documentation>Used to specify a segment number.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="Type" use="required">
                                    <annotation>
                                      <documentation>Used to specify the type of remark.
                                                                                Acceptable values are:
                                                                                Alpha-Coded,
                                                                                Client Address,
                                                                                Corporate,
                                                                                Delivery Address,
                                                                                General,
                                                                                Group Name,
                                                                                Hidden,
                                                                                Historical,
                                                                                Invoice,
                                                                                Itinerary.</documentation>
                                    </annotation>
                                    <simpleType>
                                      <restriction base="string">
                                        <enumeration value="Alpha-Coded" />
                                        <enumeration value="Client Address" />
                                        <enumeration value="Corporate" />
                                        <enumeration value="Delivery Address" />
                                        <enumeration value="General" />
                                        <enumeration value="Group Name" />
                                        <enumeration value="Hidden" />
                                        <enumeration value="Historical" />
                                        <enumeration value="Invoice" />
                                        <enumeration value="Itinerary" />
                                      </restriction>
                                    </simpleType>
                                  </attribute>
                                </complexType>
                              </element>
                            </sequence>
                          </complexType>
                        </element>
                      </sequence>
                    </complexType>
                  </element>
                  <element minOccurs="0" name="AirSeatRQ">
                    <annotation>
                      <documentation>Used to book seats.</documentation>
                    </annotation>
                    <complexType>
                      <sequence>
                        <element name="Seats">
                          <annotation>
                            <documentation>Used to pass seat details.</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element maxOccurs="98" name="Seat">
                                <annotation>
                                  <documentation>Used for a single seat request.</documentation>
                                </annotation>
                                <complexType>
                                  <attribute name="BoardingPass" type="boolean">
                                    <annotation>
                                      <documentation>Used to specify for the system to assign a seat and issue a boarding pass.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="ChangeOfGauge" type="boolean">
                                    <annotation>
                                      <documentation>Used to specify a change of gauge of flight.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="NameNumber" type="string" use="required">
                                    <annotation>
                                      <documentation>Used to specify a passenger name number.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="Number" type="string">
                                    <annotation>
                                      <documentation>Used to specify a seat number.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="Preference" type="string">
                                    <annotation>
                                      <documentation>Used to specify a seat preference.
                                                                                Preference cannot combine with Number.</documentation>
                                    </annotation>
                                  </attribute>
                                  <attribute name="SegmentNumber" type="string">
                                    <annotation>
                                      <documentation>Used to specify a segment number.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                            </sequence>
                          </complexType>
                        </element>
                      </sequence>
                    </complexType>
                  </element>
                  <element minOccurs="0" name="SpecialServiceRQ">
                    <annotation>
                      <documentation>Used to add SSRs.</documentation>
                    </annotation>
                    <complexType>
                      <sequence>
                        <element name="SpecialServiceInfo">
                          <annotation>
                            <documentation>Used to pass SSR details.</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element minOccurs="0" maxOccurs="unbounded" name="AdvancePassenger">
                                <annotation>
                                  <documentation>Used to specify the APIS (Advance Passenger Information).</documentation>
                                </annotation>
                                <complexType>
                                  <sequence>
                                    <element minOccurs="0" name="Document">
                                      <annotation>
                                        <documentation>Used to specify document details.</documentation>
                                      </annotation>
                                      <complexType>
                                        <sequence>
                                          <element minOccurs="0" name="IssueCountry" type="string">
                                            <annotation>
                                              <documentation>Used to pass the document country of issue.</documentation>
                                            </annotation>
                                          </element>
                                          <element minOccurs="0" name="NationalityCountry" type="string">
                                            <annotation>
                                              <documentation>Used to pass the document nationality country.</documentation>
                                            </annotation>
                                          </element>
                                          <element minOccurs="0" name="Visa">
                                            <annotation>
                                              <documentation>Used to pass visa document details.</documentation>
                                            </annotation>
                                            <complexType>
                                              <sequence>
                                                <element minOccurs="0" name="ApplicableCountry" type="string">
                                                  <annotation>
                                                    <documentation>Used to pass the visa document applicable country code.
                                                                                                            You must use a valid 2 or 3 character country code (alpha characters only).</documentation>
                                                  </annotation>
                                                </element>
                                                <element minOccurs="0" name="PlaceOfBirth" type="string">
                                                  <annotation>
                                                    <documentation>Used to pass the visa document place of birth.</documentation>
                                                  </annotation>
                                                </element>
                                                <element minOccurs="0" name="PlaceOfIssue" type="string">
                                                  <annotation>
                                                    <documentation>Used to pass the visa document place of issue.</documentation>
                                                  </annotation>
                                                </element>
                                              </sequence>
                                              <attribute name="IssueDate" type="spct:fullDate">
                                                <annotation>
                                                  <documentation>Used to specify the visa issue date.</documentation>
                                                </annotation>
                                              </attribute>
                                            </complexType>
                                          </element>
                                        </sequence>
                                        <attribute name="ExpirationDate" type="spct:fullDate">
                                          <annotation>
                                            <documentation>Used to specify the document expiration date.</documentation>
                                          </annotation>
                                        </attribute>
                                        <attribute name="Number" type="string">
                                          <annotation>
                                            <documentation>Used to specify the document number.</documentation>
                                          </annotation>
                                        </attribute>
                                        <attribute name="Type" type="string" use="required">
                                          <annotation>
                                            <documentation>Used to specify the document type.
                                                                                            Acceptable values include:
                                                                                            "A" - Alien resident card,
                                                                                            "C" - Permanent resident card,
                                                                                            "F" - Facilitation document,
                                                                                            "I" - National ID card,
                                                                                            "IN" - Nexus Card.
                                                                                            Also:
                                                                                            "M" - Military,
                                                                                            "N" - Naturalization certificate,
                                                                                            "P" - Passport,
                                                                                            "T" - Refugee travel document and re-entry permit,
                                                                                            US Travel document,
                                                                                            "V" - Border crossing card.</documentation>
                                          </annotation>
                                        </attribute>
                                      </complexType>
                                    </element>
                                    <element minOccurs="0" name="PersonName">
                                      <annotation>
                                        <documentation>Used to pass personal details.</documentation>
                                      </annotation>
                                      <complexType>
                                        <sequence>
                                          <element minOccurs="0" name="GivenName" type="string">
                                            <annotation>
                                              <documentation>Used to pass the first name.</documentation>
                                            </annotation>
                                          </element>
                                          <element minOccurs="0" name="MiddleName" type="string">
                                            <annotation>
                                              <documentation>Used to pass the middle name.</documentation>
                                            </annotation>
                                          </element>
                                          <element minOccurs="0" name="Surname" type="string">
                                            <annotation>
                                              <documentation>Used to pass the surname.</documentation>
                                            </annotation>
                                          </element>
                                        </sequence>
                                        <attribute name="DateOfBirth" type="spct:fullDate">
                                          <annotation>
                                            <documentation>Used to pass the date of birth.
                                                                                            It follows the YYYY-MM-DD format.</documentation>
