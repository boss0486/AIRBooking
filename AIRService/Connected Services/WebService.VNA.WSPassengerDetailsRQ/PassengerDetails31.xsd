                      <documentation>Used to pass agency details.</documentation>
                    </annotation>
                    <complexType>
                      <sequence>
                        <element minOccurs="0" name="Address">
                          <annotation>
                            <documentation>Used to add an agency address into the record.</documentation>
                          </annotation>
                          <complexType>
                            <sequence>
                              <element minOccurs="0" name="AddressLine" type="string">
                                <annotation>
                                  <documentation>Used to pass the agency address.</documentation>
                                </annotation>
                              </element>
                              <element name="CityName" type="string">
                                <annotation>
                                  <documentation>Used to pass the agency city name.</documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="CountryCode" type="string">
                                <annotation>
                                  <documentation>Used to pass the agency country code.</documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="PostalCode" type="string">
                                <annotation>
                                  <documentation>Used to pass the agency postal code.</documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="StateCountyProv">
                                <annotation>
                                  <documentation>Used to pass the state or country province details.</documentation>
                                </annotation>
                                <complexType>
                                  <attribute name="StateCode" type="string" use="required">
                                    <annotation>
                                      <documentation>Used to pass the state or province code.</documentation>
                                    </annotation>
                                  </attribute>
                                </complexType>
                              </element>
                              <element minOccurs="0" name="StreetNmbr" type="string">
                                <annotation>
                                  <documentation>Used to pass the agency street number.</documentation>
                                </annotation>
                              </element>
                              <element minOccurs="0" name="VendorPrefs">
                                <annotation>
                                  <documentation>Used to specify the vendor preferences.</documentation>
                                </annotation>
                                <complexType>
                                  <sequence>
                                    <element name="Airline">
                                      <annotation>
                                        <documentation>Used to specify whether the agency information should reference a hosted or non hosted airline vendor.</documentation>
                                      </annotation>
                                      <complexType>
                                        <attribute name="Hosted" type="boolean" use="required">
                                          <annotation>
                                            <documentation>Used to specify if the request is based upon a hosted or a non-hosted carrier.
                                                                                            For Travel Network customers "Hosted" should be set to "false".
                                                                                            For SabreSonic RES hosted customers "Hosted" should be set to "true".</documentation>
                                          </annotation>
                                        </attribute>
